    "https://linkedsoftwaredependencies.org/bundles/npm/@iotakingdoms/common/*/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@iotakingdoms/common/*/components/": "dist/components/",
